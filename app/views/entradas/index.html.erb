<p id="notice"><%= notice %></p>

<h1>Entradas</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Remision</th>
      <th>Articulo</th>
      <th>Cantidad</th>
      <th>Recibe</th>
      <th>Proveedor</th>
      <th>Autoriza</th>
      <th>Solicita</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entradas.each do |entrada| %>
      <tr>
        <td><%= entrada.fecha %></td>
        <td><%= entrada.remision %></td>
        <td><%= entrada.articulo %></td>
        <td><%= entrada.cantidad %></td>
        <td><%= entrada.recibe %></td>
        <td><%= entrada.proveedor %></td>
        <td><%= entrada.autoriza %></td>
        <td><%= entrada.solicita %></td>
        <td><%= link_to 'Show', entrada %></td>
        <td><%= link_to 'Edit', edit_entrada_path(entrada) %></td>
        <td><%= link_to 'Destroy', entrada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entrada', new_entrada_path %>
